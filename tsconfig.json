{
  "compilerOptions": {
    "lib": [
      "es2015",
      "es2016",
      "es2017"
    ],
    "strictNullChecks": true,
    "sourceMap": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "es5"
  },
  "include": [
    "."
  ],
  "exclude": [
    "./node_modules"
  ],
  "compileOnSave": false,
  "atom": {
    "rewriteTsconfig": false
  }
}